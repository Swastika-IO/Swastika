@model Sio.Cms.Lib.ViewModels.SioModules.ReadMvcViewModel
<!-- custom render view here -->    
    <div class="buttons">
        @if (Model!=null)
        {
            @foreach (var item in Model.Data.JsonItems)
            {
            <a href="@item["url"]["value"]" class="btn btn-icon btn-link btn-neutral btn-lg">
                <i class="fab @item["icon"]["value"]"></i>
            </a>
            }
        }
    </div>
